package String;

public class StringTest {
	public static void main(String[] args) {
		String s="123;456;789;123";
		System.out.println(s.charAt(2));
		//System.out.println(s.indexOf(";"));
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
	}
}
